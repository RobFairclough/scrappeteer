{
  "name": "scrappeteer",
  "description": "Puppeteer project template made for web scraping",
  "version": "0.1.0",
  "repository": "git@github.com:grikomsn/scrappeteer.git",
  "author": "Griko Nibras <grikomsn@gmail.com>",
  "homepage": "https://github.com/grikomsn/scrappeteer",
  "bugs": "https://github.com/grikomsn/scrappeteer/issues",
  "scripts": {
    "format": "prettier --config package.json --write src/**/*.ts",
    "start": "ts-node src/app.ts",
    "start:local": "AWS_EXECUTION_ENV = \"AWS_Lambda_nodejs8.10\" AWS_LAMBDA_FUNCTION_NAME = \"scrappeteer\" ts-node src/app.ts"
  },
  "dependencies": {
    "@types/puppeteer-core": "^1.9.0",
    "chrome-aws-lambda": "^1.20.3",
    "papaparse": "^5.1.0",
    "puppeteer-core": "^1.20.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3"
  },
  "devDependencies": {
    "@types/papaparse": "^5.0.1",
    "husky": "^3.0.8",
    "prettier": "^1.18.2",
    "puppeteer": "^1.20.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "keywords": [
    "scrappeteer",
    "puppeteer",
    "chrome-aws-lambda",
    "web scraper",
    "web scraping",
    "scraper",
    "scraping"
  ],
  "license": "MIT"
}
